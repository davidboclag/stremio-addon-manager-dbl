{
  "version": 2,
  "framework": null,
  "buildCommand": "npm run build:prod",
  "outputDirectory": "dist/stremio-addon-manager-dbl/browser",
  "installCommand": "npm install",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "git": {
    "deploymentEnabled": {
      "main": true,
      "develop": true
    },
    "productionBranch": "main"
  }
}
